{
  "name": "react-motion-ts",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.8.0",
  "description": "A spring that solves your animation problems.",
  "author": "Blithe07 <Blithe0712@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Blithe07/react-motion-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blithe07/react-motion-ts.git"
  },
  "bugs": "https://github.com/Blithe07/react-motion-ts/issues",
  "keywords": [
    "react",
    "component",
    "react-component",
    "transitiongroup",
    "spring",
    "tween",
    "motion",
    "animation",
    "transition",
    "ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "performance-now": "^2.1.0",
    "prop-types": "^15.5.8",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.22",
    "@antfu/ni": "^0.21.8",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.8.3",
    "@types/raf": "^3.4.1",
    "@types/react": "^18.2.28",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.8.0",
    "react": "^18.2.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
